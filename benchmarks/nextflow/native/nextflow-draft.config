    freq        = 20  // (MANDATORY integer)  
    freq2       = 40  // (MANDATORY integer)  
    freq3       = 50  // (MANDATORY integer)  
    inp_file    = 'INPUT_FILE'  // (MANDATORY string)   
    out_pic     = 'out_fin.png'  // (MANDATORY string)   
    out_segy    = 'filtered.segy'  // (MANDATORY string)   
