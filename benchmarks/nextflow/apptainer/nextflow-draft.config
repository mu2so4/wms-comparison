
nextflow.enable.dsl = 2
apptainer.enabled = true
apptainer.autoMounts = true
apptainer.cacheDir = "$HOME/.singularity/cache"

params {
    
    // Placeholder for null values.
    // Do not alter unless you know what you are doing.
    NULL_VALUE = 'NULL'

    // WORKFLOW OUTPUT DIRECTORY
    outdir  = './outputs'

    // INPUTS (MANDATORY)
    inp_file    = 'INPUT_FILE'  // (MANDATORY generic file)  
    f1          = 15  // (MANDATORY integer)       
    f2          = 30  // (MANDATORY integer)       
    freq        = 20  // (MANDATORY integer)       
    freq2       = 40  // (MANDATORY integer)       
    freq3       = 50  // (MANDATORY integer)       
    out_pic     = 'pic.png'  // (MANDATORY string)        
    out_segy    = 'out.segy'  // (MANDATORY string)        
    out_stage1  = 'f.sd'  // (MANDATORY string)        


}
